{
  multimap().entries().retainAll(Collections.singleton(Helpers.mapEntry(sampleKeys().e0,sampleValues().e0)));
  assertEquals(getSubjectGenerator().create(Helpers.mapEntry(sampleKeys().e0,sampleValues().e0)),multimap());
  assertEquals(1,multimap().size());
  assertTrue(multimap().containsEntry(sampleKeys().e0,sampleValues().e0));
}
