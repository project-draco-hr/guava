{
  lock.lock();
  try {
    if (state != State.STARTING) {
      IllegalStateException failure=new IllegalStateException("Cannot notifyStarted() when the service is " + state);
      notifyFailed(failure);
      throw failure;
    }
    if (shutdownWhenStartupFinishes) {
      state=State.STOPPING;
      doStop();
    }
 else {
      state=State.RUNNING;
      running();
    }
  }
  finally {
    lock.unlock();
    executeListeners();
  }
}
