{
  String jacksons="arfo(Marlon)aorf, (Michael)orfa, afro(Jackie)orfa, " + "ofar(Jemaine), aff(Tito)";
  Iterable<String> family=Splitter.on(',').trimResults(CharMatcher.anyOf("afro").or(CharMatcher.WHITESPACE)).split(jacksons);
  assertThat(family).iteratesAs("(Marlon)","(Michael)","(Jackie)","(Jemaine)","(Tito)");
}
