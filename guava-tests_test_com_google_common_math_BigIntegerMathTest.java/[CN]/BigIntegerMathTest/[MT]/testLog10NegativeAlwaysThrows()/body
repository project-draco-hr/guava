{
  for (  BigInteger x : POSITIVE_BIGINTEGER_CANDIDATES) {
    for (    RoundingMode mode : ALL_ROUNDING_MODES) {
      try {
        BigIntegerMath.log10(x.negate(),mode);
        fail("Expected IllegalArgumentException");
      }
 catch (      IllegalArgumentException expected) {
      }
    }
  }
}
