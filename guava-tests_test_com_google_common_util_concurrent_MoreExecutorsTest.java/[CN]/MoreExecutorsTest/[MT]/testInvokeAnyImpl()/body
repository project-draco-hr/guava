{
  ListeningExecutorService e=newDirectExecutorService();
  try {
    List<Callable<String>> l=new ArrayList<Callable<String>>();
    l.add(new StringTask());
    l.add(new StringTask());
    String result=invokeAnyImpl(e,l,false,0);
    assertSame(TEST_STRING,result);
  }
  finally {
    joinPool(e);
  }
}
