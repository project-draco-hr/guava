{
  if (g1 == g2) {
    return true;
  }
  if (g1 == null || g2 == null) {
    return false;
  }
  if (!g1.nodes().equals(g2.nodes()) || !g1.edges().equals(g2.edges())) {
    return false;
  }
  for (  Object e : g1.edges()) {
    if (!g1.source(e).equals(g2.source(e)) || !g1.target(e).equals(g2.target(e))) {
      return false;
    }
  }
  return true;
}
