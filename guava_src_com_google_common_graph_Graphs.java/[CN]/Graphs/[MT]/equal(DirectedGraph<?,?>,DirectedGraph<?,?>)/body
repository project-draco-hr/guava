{
  if (graph1 == graph2) {
    return true;
  }
  if (graph1 == null || graph2 == null) {
    return false;
  }
  if (!graph1.nodes().equals(graph2.nodes()) || !graph1.edges().equals(graph2.edges())) {
    return false;
  }
  for (  Object edge : graph1.edges()) {
    if (!graph1.source(edge).equals(graph2.source(edge)) || !graph1.target(edge).equals(graph2.target(edge))) {
      return false;
    }
  }
  return true;
}
