{
  if (g1 == g2) {
    return true;
  }
  if (g1 == null || g2 == null) {
    return false;
  }
  if (!g1.nodes().equals(g2.nodes()) || !g1.edges().equals(g2.edges())) {
    return false;
  }
  for (  Object e : g1.edges()) {
    if (!g1.incidentNodes(e).equals(g2.incidentNodes(e))) {
      return false;
    }
  }
  return true;
}
