{
  Multimap<String,Integer> multimap=create();
  multimap.put("foo",1);
  multimap.put("foo",3);
  multimap.put("bar",3);
  multimap.put("foo",1);
  assertEquals(4,multimap.size());
  assertTrue(multimap.containsEntry("foo",1));
  multimap.remove("foo",1);
  assertEquals(3,multimap.size());
  assertTrue(multimap.containsEntry("foo",1));
}
