{
  Class<? super T> rawType=type.getRawType();
  @SuppressWarnings("unchecked") T defaultValue=(T)defaultValues.getInstance(rawType);
  if (defaultValue != null) {
    return defaultValue;
  }
  @SuppressWarnings("unchecked") T value=(T)ArbitraryInstances.get(rawType);
  if (value != null) {
    return value;
  }
  if (rawType.isInterface()) {
    return new DummyProxy(){
      @Override <R>R dummyReturnValue(      TypeToken<R> returnType){
        return getDummyValue(returnType);
      }
    }
.newProxy(type);
  }
  return null;
}
