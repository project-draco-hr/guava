{
  if (obj == this) {
    return true;
  }
  if (!(obj instanceof Graph)) {
    return false;
  }
  Graph<?,?> other=(Graph<?,?>)obj;
  if (isDirected() != other.isDirected() || !nodes().equals(other.nodes()) || !edges().equals(other.edges())) {
    return false;
  }
  for (  Endpoints<N> edge : edges()) {
    if (!edgeValue(edge.nodeA(),edge.nodeB()).equals(other.edgeValue(edge.nodeA(),edge.nodeB()))) {
      return false;
    }
  }
  return true;
}
