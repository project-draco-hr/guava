{
  assertEquals(0,Utf8.utf8Length(""));
  assertEquals(11,Utf8.utf8Length("Hello world"));
  assertEquals(8,Utf8.utf8Length("R??sum??"));
  assertEquals(461,Utf8.utf8Length("???????????????????????William Shakespeare???" + "1564???4???26??????1616???4???23???[1]????????????????????????????????????????????????" + "?????????????????????????????????????????????????????????????????????????????????????????????????????????"+ "????????????????????????????????????????????????????????????????????????????????????????????????"+ "???????????????[2]??????????????????????????????????????????????????????????????????????????????"+ "?????????????????????????????????"));
  assertEquals(4,Utf8.utf8Length(newString(Character.MIN_HIGH_SURROGATE,Character.MIN_LOW_SURROGATE)));
}
