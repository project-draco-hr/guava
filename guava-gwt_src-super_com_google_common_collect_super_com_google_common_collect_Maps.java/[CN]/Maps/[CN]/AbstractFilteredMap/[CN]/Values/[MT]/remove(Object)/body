{
  Iterator<Entry<K,V>> unfilteredItr=unfiltered.entrySet().iterator();
  while (unfilteredItr.hasNext()) {
    Entry<K,V> entry=unfilteredItr.next();
    if (predicate.apply(entry) && Objects.equal(entry.getValue(),o)) {
      unfilteredItr.remove();
      return true;
    }
  }
  return false;
}
