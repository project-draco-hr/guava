{
  lock();
  try {
    for (ReferenceEntry<K,V> e=getFirst(hash); e != null; e=e.getNext()) {
      K entryKey=e.getKey();
      if (e.getHash() == hash && entryKey != null && map.keyEquivalence.equivalent(key,entryKey)) {
        ValueReference<K,V> v=e.getValueReference();
        if (v == valueReference) {
          enqueueCleanup(e);
          return true;
        }
        return false;
      }
    }
    return false;
  }
  finally {
    unlock();
    postWriteCleanup();
  }
}
