{
  try {
    K key=entry.getKey();
    V value=entry.getValueReference().get();
    if (key != null && value != null && !(expires() && isExpired(entry))) {
      nextExternal=new WriteThroughEntry(key,value);
      return true;
    }
 else {
      return false;
    }
  }
  finally {
    currentSegment.postReadCleanup();
  }
}
