{
  Preconditions.checkArgument(numHashFunctions > 0,"numHashFunctions zero or negative");
  this.bits=checkNotNull(bits);
  this.numHashFunctions=numHashFunctions;
  this.funnel=checkNotNull(funnel);
  this.strategy=strategy;
  if (numHashFunctions > 255) {
    throw new AssertionError("Currently we don't allow BloomFilters that would use more than" + "255 hash functions, please contact the guava team");
  }
}
