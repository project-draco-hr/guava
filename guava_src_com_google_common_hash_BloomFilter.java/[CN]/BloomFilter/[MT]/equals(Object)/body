{
  if (object == this) {
    return true;
  }
  if (object instanceof BloomFilter) {
    BloomFilter<?> that=(BloomFilter<?>)object;
    return this.numHashFunctions == that.numHashFunctions && this.funnel == that.funnel && this.strategy == that.strategy && this.bits.equals(that.bits);
  }
  return false;
}
