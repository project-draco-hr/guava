{
  new EqualsTester().addEqualityGroup(MediaType.create("text","plain")).addEqualityGroup(MediaType.create("text","plain").withCharset(UTF_8),MediaType.create("text","plain").withParameter("CHARSET","UTF-8"),MediaType.create("text","plain").withParameters(ImmutableMultimap.of("charset","utf-8")),MediaType.parse("text/plain; charset=utf-8"),MediaType.parse("text/plain; CHARSET=utf-8"),MediaType.parse("text/plain; charset=\"utf-8\""),MediaType.parse("text/plain; charset=\"\\u\\tf-\\8\""),MediaType.parse("text/plain; charset=UTF-8")).addEqualityGroup(MediaType.create("text","plain").withCharset(UTF_16)).testEquals();
}
