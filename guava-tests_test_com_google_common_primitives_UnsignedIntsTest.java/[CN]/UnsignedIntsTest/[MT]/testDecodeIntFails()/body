{
  try {
    int unused=UnsignedInts.decode("0xfffffffff");
    fail();
  }
 catch (  NumberFormatException expected) {
  }
  try {
    int unused=UnsignedInts.decode("-5");
    fail();
  }
 catch (  NumberFormatException expected) {
  }
  try {
    int unused=UnsignedInts.decode("-0x5");
    fail();
  }
 catch (  NumberFormatException expected) {
  }
  try {
    int unused=UnsignedInts.decode("-05");
    fail();
  }
 catch (  NumberFormatException expected) {
  }
}
