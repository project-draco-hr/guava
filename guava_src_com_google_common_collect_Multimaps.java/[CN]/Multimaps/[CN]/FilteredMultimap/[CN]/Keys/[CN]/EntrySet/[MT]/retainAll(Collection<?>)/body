{
  Predicate<Map.Entry<K,Collection<V>>> removalPredicate=new Predicate<Map.Entry<K,Collection<V>>>(){
    @Override public boolean apply(    Map.Entry<K,Collection<V>> entry){
      Multiset.Entry<K> multisetEntry=Multisets.immutableEntry(entry.getKey(),entry.getValue().size());
      return !c.contains(multisetEntry);
    }
  }
;
  return removeEntriesIf(removalPredicate);
}
