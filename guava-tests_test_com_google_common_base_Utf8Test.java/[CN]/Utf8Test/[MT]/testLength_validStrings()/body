  assertEquals(461,Utf8.length("???????????????????????William Shakespeare???" + "1564???4???26??????1616???4???23???[1]????????????????????????????????????????????????" + "?????????????????????????????????????????????????????????????????????????????????????????????????????????"+ "????????????????????????????????????????????????????????????????????????????????????????????????"+ "???????????????[2]??????????????????????????????????????????????????????????????????????????????"+ "?????????????????????????????????"));
  assertEquals(4,Utf8.length(newString(Character.MIN_HIGH_SURROGATE,Character.MIN_LOW_SURROGATE)));
}
