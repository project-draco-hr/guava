{
  assertValidUtf8();
  assertValidUtf8(0x00,0x61,0x62,0x63,0x7F);
  assertValidUtf8(0xC2,0xA2,0xC2,0xA2);
  assertValidUtf8(0xc8,0x8a,0x63,0xc8,0x8a,0x63);
  assertValidUtf8(0xc9,0x8b,0x36,0x32,0xc9,0x8b,0x36,0x32);
  assertValidUtf8(0x61,0xc8,0x8a,0x63,0xc2,0xa2,0x62,0x5c,0x75,0x30,0x32,0x34,0x42,0x36,0x32,0x75,0x30,0x32,0x30,0x61,0x63,0x63,0xc2,0xa2,0x64,0x65,0xc9,0x8b,0x36,0x32);
  assertInvalidUtf8(-1,0,-1,0);
}
