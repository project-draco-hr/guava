{
  if (object == this) {
    return true;
  }
  if (object instanceof Map) {
    Map<?,?> that=(Map<?,?>)object;
    if (that.size() == 1) {
      Entry<?,?> entry=that.entrySet().iterator().next();
      return singleKey.equals(entry.getKey()) && singleValue.equals(entry.getValue());
    }
  }
  return false;
}
