{
  BasicGraphBuilder<Integer> builder=BasicGraphBuilder.from(graph);
  MutableBasicGraph<Integer> g1=builder.build();
  MutableBasicGraph<Integer> g2=builder.build();
  g1.putEdge(N1,N2);
  g1.putEdge(N3,N1);
  g2.putEdge(N3,N1);
  g2.putEdge(N1,N2);
  new EqualsTester().addEqualityGroup(g1,g2).testEquals();
}
