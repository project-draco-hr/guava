{
  Set<Feature<?>> features;
  features=Sets.<Feature<?>>newHashSet(ExampleDerivedFeature.DERIVED_FEATURE_1);
  assertTrue(FeatureUtil.impliedFeatures(features).isEmpty());
  features=Sets.<Feature<?>>newHashSet(ExampleDerivedFeature.DERIVED_FEATURE_2);
  ASSERT.that(FeatureUtil.impliedFeatures(features)).hasContentsAnyOrder(ExampleBaseFeature.BASE_FEATURE_1);
  features=Sets.<Feature<?>>newHashSet(ExampleDerivedFeature.COMPOUND_DERIVED_FEATURE);
  ASSERT.that(FeatureUtil.impliedFeatures(features)).hasContentsAnyOrder(ExampleDerivedFeature.DERIVED_FEATURE_1,ExampleDerivedFeature.DERIVED_FEATURE_2,ExampleBaseFeature.BASE_FEATURE_1,ExampleBaseFeature.BASE_FEATURE_2);
}
