{
  multimap.put("foo",1);
  multimap.put("foo",nullValue());
  multimap.put(nullKey(),3);
  Collection<Entry<String,Integer>> entries=multimap.entries();
  ASSERT.that(entries).hasContentsAnyOrder(Maps.immutableEntry("foo",1),Maps.immutableEntry("foo",nullValue()),Maps.immutableEntry(nullKey(),3));
}
