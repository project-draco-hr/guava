{
  Multimap<String,Integer> map=create();
  map.put("bar",1);
  map.put("foo",2);
  map.put("bar",3);
  map.put("bar",4);
  assertEquals("[bar=1, foo=2, bar=3, bar=4]",map.entries().toString());
  ASSERT.that(map.keys()).has().allOf("bar","foo","bar","bar").inOrder();
  map.keys().remove("bar");
  assertEquals("{foo=[2], bar=[3, 4]}",map.toString());
}
