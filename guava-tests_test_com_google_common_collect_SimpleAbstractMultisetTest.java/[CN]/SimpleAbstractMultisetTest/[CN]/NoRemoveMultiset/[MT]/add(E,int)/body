{
  checkArgument(occurrences >= 0);
  Integer frequency=backingMap.get(element);
  if (frequency == null) {
    frequency=0;
  }
  if (occurrences == 0) {
    return frequency;
  }
  checkArgument(occurrences <= Integer.MAX_VALUE - frequency);
  backingMap.put(element,frequency + occurrences);
  return frequency;
}
