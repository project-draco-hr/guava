{
  if (parameterIsPrimitiveOrNullable(func,paramIndex)) {
    return;
  }
  Object[] params=buildParamList(func,paramIndex);
  try {
    func.invoke(instance,params);
    GuavaAsserts.fail("No exception thrown from " + func + Arrays.toString(params)+ " for "+ testedClass);
  }
 catch (  InvocationTargetException e) {
    Throwable cause=e.getCause();
    GuavaAsserts.assertTrue("wrong exception thrown from " + func + ": "+ cause,cause instanceof NullPointerException || cause instanceof UnsupportedOperationException);
  }
}
