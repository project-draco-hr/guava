{
  checkNotNull(future);
  try {
    return getUninterruptibly(future);
  }
 catch (  CancellationException e) {
    throw e;
  }
catch (  ExecutionException e) {
    throw new UncheckedExecutionException(e.getCause());
  }
catch (  RuntimeException e) {
    throw new UncheckedExecutionException(e);
  }
}
