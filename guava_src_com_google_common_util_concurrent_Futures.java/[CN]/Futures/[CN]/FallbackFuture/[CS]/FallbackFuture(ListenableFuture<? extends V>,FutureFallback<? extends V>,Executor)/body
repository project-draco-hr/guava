{
  running=input;
  addCallback(running,new FutureCallback<V>(){
    @Override public void onSuccess(    V value){
      set(value);
    }
    @Override public void onFailure(    Throwable t){
      if (isCancelled()) {
        return;
      }
      try {
        setFuture(fallback.create(t));
      }
 catch (      Throwable e) {
        setException(e);
      }
    }
  }
,executor);
}
