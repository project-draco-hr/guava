{
  Table<Character,Integer,String> table=TreeBasedTable.create();
  validateTableCopies(table);
  table.put('b',2,"foo");
  validateTableCopies(table);
  table.put('b',1,"bar");
  table.put('a',2,"baz");
  validateTableCopies(table);
  ASSERT.that(table.row('b').keySet()).has().allOf(1,2).inOrder();
  ASSERT.that(ImmutableTable.copyOf(table).row('b').keySet()).has().allOf(2,1).inOrder();
}
