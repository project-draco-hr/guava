{
  new EqualsTester().addEqualityGroup(Predicates.and(TRUE,isOdd(),NEVER_REACHED),Predicates.and(TRUE,isOdd(),NEVER_REACHED)).addEqualityGroup(Predicates.and(isOdd(),NEVER_REACHED,TRUE)).addEqualityGroup(Predicates.and(TRUE)).addEqualityGroup(Predicates.or(TRUE,isOdd(),NEVER_REACHED)).testEquals();
}
