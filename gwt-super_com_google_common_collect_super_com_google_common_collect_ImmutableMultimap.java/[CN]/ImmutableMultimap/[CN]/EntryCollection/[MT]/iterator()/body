{
  final Iterator<? extends Map.Entry<K,? extends ImmutableCollection<V>>> mapIterator=this.multimap.map.entrySet().iterator();
  return new UnmodifiableIterator<Map.Entry<K,V>>(){
    K key;
    Iterator<V> valueIterator;
    @Override public boolean hasNext(){
      return (key != null && valueIterator.hasNext()) || mapIterator.hasNext();
    }
    @Override public Map.Entry<K,V> next(){
      if (key == null || !valueIterator.hasNext()) {
        Map.Entry<K,? extends ImmutableCollection<V>> entry=mapIterator.next();
        key=entry.getKey();
        valueIterator=entry.getValue().iterator();
      }
      return Maps.immutableEntry(key,valueIterator.next());
    }
  }
;
}
