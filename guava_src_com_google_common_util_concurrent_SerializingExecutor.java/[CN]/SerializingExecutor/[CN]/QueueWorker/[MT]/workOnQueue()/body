{
  while (true) {
    Runnable task=null;
synchronized (internalLock) {
      if (suspensions == 0 && !Thread.currentThread().isInterrupted()) {
        task=queue.poll();
      }
      if (task == null) {
        isWorkerRunning=false;
        return;
      }
    }
    try {
      task.run();
    }
 catch (    RuntimeException e) {
      log.log(Level.SEVERE,"Exception while executing runnable " + task,e);
    }
  }
}
