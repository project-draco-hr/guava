{
  RemovalListener<Object,Object> testListener=new RemovalListener<Object,Object>(){
    @Override public void onRemoval(    RemovalNotification<Object,Object> notification){
    }
  }
;
  MapMakerInternalMap<Object,Object> map=makeMap(createMapMaker().removalListener(testListener));
  assertSame(testListener,map.removalListener);
}
