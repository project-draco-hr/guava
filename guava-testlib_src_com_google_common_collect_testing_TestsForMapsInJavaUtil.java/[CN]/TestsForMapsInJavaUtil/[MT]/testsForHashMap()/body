{
  return MapTestSuiteBuilder.using(new TestStringMapGenerator(){
    @Override protected Map<String,String> create(    Entry<String,String>[] entries){
      return toHashMap(entries);
    }
  }
).named("HashMap").withFeatures(MapFeature.GENERAL_PURPOSE,MapFeature.ALLOWS_NULL_KEYS,MapFeature.ALLOWS_NULL_VALUES,MapFeature.FAILS_FAST_ON_CONCURRENT_MODIFICATION,CollectionFeature.SUPPORTS_ITERATOR_REMOVE,CollectionFeature.SERIALIZABLE,CollectionSize.ANY).suppressing(suppressForHashMap()).createTestSuite();
}
