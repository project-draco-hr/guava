{
  CharSource c1=CharStreams.asCharSource("abc");
  CharSource c2=CharStreams.asCharSource("");
  CharSource c3=CharStreams.asCharSource("de");
  String expected="abcde";
  assertEquals(expected,CharStreams.concat(ImmutableList.of(c1,c2,c3)).read());
  assertEquals(expected,CharStreams.concat(c1,c2,c3).read());
  assertEquals(expected,CharStreams.concat(ImmutableList.of(c1,c2,c3).iterator()).read());
}
