{
  NoOpService service=new NoOpService();
  currentThread().interrupt();
  try {
    service.startAndWait();
    assertEquals(State.RUNNING,service.state());
    service.stopAndWait();
    assertEquals(State.TERMINATED,service.state());
    assertTrue(currentThread().isInterrupted());
  }
  finally {
    Thread.interrupted();
  }
}
