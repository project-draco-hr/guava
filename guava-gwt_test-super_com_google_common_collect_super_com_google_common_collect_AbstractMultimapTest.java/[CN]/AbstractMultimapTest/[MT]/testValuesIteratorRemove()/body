{
  multimap.put("foo",1);
  multimap.put("foo",2);
  multimap.put(nullKey(),4);
  Iterator<Integer> iterator=multimap.values().iterator();
  while (iterator.hasNext()) {
    int value=iterator.next();
    if ((value % 2) == 0) {
      iterator.remove();
    }
  }
  assertSize(1);
  assertTrue(multimap.containsEntry("foo",1));
}
