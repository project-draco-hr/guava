{
  multimap.put("foo",1);
  multimap.put("foo",2);
  multimap.put("bar",3);
  assertFalse(multimap.entries().retainAll(asList(Maps.immutableEntry("foo",1),Maps.immutableEntry("foo",2),Maps.immutableEntry("foo",3),Maps.immutableEntry("bar",3))));
  assertSize(3);
  assertTrue(multimap.entries().retainAll(asList(Maps.immutableEntry("foo",3),Maps.immutableEntry("bar",3))));
  assertSize(1);
  assertTrue(multimap.containsEntry("bar",3));
}
