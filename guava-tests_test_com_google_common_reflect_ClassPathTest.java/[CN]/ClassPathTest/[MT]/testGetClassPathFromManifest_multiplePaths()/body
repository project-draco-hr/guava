{
  File jarFile=new File("base/some.jar");
  Manifest manifest=manifestClasspath("file:/with/absolute.jar relative.jar  relative/dir");
  assertThat(ClassPath.Scanner.getClassPathFromManifest(jarFile,manifest)).has().exactly(new File("/with/absolute.jar").toURI(),new File("base/relative.jar").toURI(),new File("base/relative/dir").toURI()).inOrder();
}
