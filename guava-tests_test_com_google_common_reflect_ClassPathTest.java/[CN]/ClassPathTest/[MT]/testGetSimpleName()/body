{
  assertEquals("Foo",classInfo("Foo.class",getClass().getClassLoader()).getSimpleName());
  assertEquals("Foo",classInfo("a/b/Foo.class",getClass().getClassLoader()).getSimpleName());
  assertEquals("Foo",classInfo("a/b/Bar$Foo.class",getClass().getClassLoader()).getSimpleName());
  assertEquals("",classInfo("a/b/Bar$1.class",getClass().getClassLoader()).getSimpleName());
  assertEquals("Foo",classInfo("a/b/Bar$Foo.class",getClass().getClassLoader()).getSimpleName());
  assertEquals("",classInfo("a/b/Bar$1.class",getClass().getClassLoader()).getSimpleName());
  assertEquals("Local",classInfo("a/b/Bar$1Local.class",getClass().getClassLoader()).getSimpleName());
}
