{
  ClassLoader classLoader=getClass().getClassLoader();
  assertEquals("Foo",new ClassInfo("Foo.class",TEST_URLS,classLoader).getSimpleName());
  assertEquals("Foo",new ClassInfo("a/b/Foo.class",TEST_URLS,classLoader).getSimpleName());
  assertEquals("Foo",new ClassInfo("a/b/Bar$Foo.class",TEST_URLS,classLoader).getSimpleName());
  assertEquals("",new ClassInfo("a/b/Bar$1.class",TEST_URLS,classLoader).getSimpleName());
  assertEquals("Foo",new ClassInfo("a/b/Bar$Foo.class",TEST_URLS,classLoader).getSimpleName());
  assertEquals("",new ClassInfo("a/b/Bar$1.class",TEST_URLS,classLoader).getSimpleName());
  assertEquals("Local",new ClassInfo("a/b/Bar$1Local.class",TEST_URLS,classLoader).getSimpleName());
}
