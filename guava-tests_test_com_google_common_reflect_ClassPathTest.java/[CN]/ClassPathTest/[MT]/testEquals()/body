{
  new EqualsTester().addEqualityGroup(classInfo(ClassPathTest.class),classInfo(ClassPathTest.class)).addEqualityGroup(classInfo(Test.class),classInfo(Test.class,getClass().getClassLoader())).addEqualityGroup(new ResourceInfo("a/b/c.txt",TEST_URLS,getClass().getClassLoader()),new ResourceInfo("a/b/c.txt",TEST_URLS,getClass().getClassLoader())).addEqualityGroup(new ResourceInfo("x.txt",TEST_URLS,getClass().getClassLoader())).testEquals();
