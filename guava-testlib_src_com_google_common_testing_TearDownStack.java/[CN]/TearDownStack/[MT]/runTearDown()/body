{
  List<Throwable> exceptions=new ArrayList<Throwable>();
  for (  TearDown tearDown : stack) {
    try {
      tearDown.tearDown();
    }
 catch (    Throwable t) {
      if (suppressThrows) {
        TearDownStack.logger.log(Level.INFO,"exception thrown during tearDown: " + t.getMessage(),t);
      }
 else {
        exceptions.add(t);
      }
    }
  }
  stack.clear();
  if ((!suppressThrows) && (exceptions.size() > 0)) {
    throw ClusterException.create(exceptions);
  }
}
