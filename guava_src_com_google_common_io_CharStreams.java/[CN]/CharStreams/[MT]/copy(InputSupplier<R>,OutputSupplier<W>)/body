{
  int successfulOps=0;
  R in=from.getInput();
  try {
    W out=to.getOutput();
    try {
      long count=copy(in,out);
      successfulOps++;
      return count;
    }
  finally {
      Closeables.close(out,successfulOps < 1);
      successfulOps++;
    }
  }
  finally {
    Closeables.close(in,successfulOps < 2);
  }
}
