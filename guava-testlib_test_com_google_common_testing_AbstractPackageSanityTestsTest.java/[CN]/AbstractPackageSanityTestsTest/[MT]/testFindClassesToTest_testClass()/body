{
  ASSERT.that(findClassesToTest(ImmutableList.of(EmptyTest.class))).isEmpty();
  ASSERT.that(findClassesToTest(ImmutableList.of(EmptyTests.class))).isEmpty();
  ASSERT.that(findClassesToTest(ImmutableList.of(EmptyTestCase.class))).isEmpty();
  ASSERT.that(findClassesToTest(ImmutableList.of(EmptyTestSuite.class))).isEmpty();
}
