{
  Closer closer=Closer.create();
  try {
    InputStream in=closer.add(openStream());
    return countBySkipping(in);
  }
 catch (  IOException e) {
  }
 finally {
    closer.close();
  }
  closer=Closer.create();
  try {
    InputStream in=closer.add(openStream());
    return countByReading(in);
  }
 catch (  Throwable e) {
    throw closer.rethrow(e,IOException.class);
  }
 finally {
    closer.close();
  }
}
