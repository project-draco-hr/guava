{
  byte[] b1="????????????".getBytes(Charsets.US_ASCII);
  byte[] b2="????????????".getBytes(Charsets.US_ASCII);
  byte[] b3="????".getBytes(Charsets.US_ASCII);
  byte[] b4="????????????".getBytes(Charsets.US_ASCII);
  byte[] b5="????????????".getBytes(Charsets.US_ASCII);
  assertTrue(Arrays.equals(b1,b2));
  assertTrue(Arrays.equals(b2,b3));
  assertTrue(Arrays.equals(b3,b4));
  assertTrue(Arrays.equals(b4,b5));
}
