{
  assertEquals(Hashing.md5().bits() + Hashing.md5().bits(),Hashing.concatenating(Hashing.md5(),Hashing.md5()).bits());
  assertEquals(Hashing.md5().bits() + Hashing.murmur3_32().bits(),Hashing.concatenating(Hashing.md5(),Hashing.murmur3_32()).bits());
  assertEquals(Hashing.md5().bits() + Hashing.murmur3_32().bits() + Hashing.murmur3_128().bits(),Hashing.concatenating(Hashing.md5(),Hashing.murmur3_32(),Hashing.murmur3_128()).bits());
}
