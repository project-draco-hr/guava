{
  if (count != 0) {
    for (ReferenceEntry<K,V> e=getFirst(hash); e != null; e=e.getNext()) {
      if (e.getHash() != hash) {
        continue;
      }
      K entryKey=e.getKey();
      if (entryKey == null) {
        continue;
      }
      if (keyEquivalence.equivalent(entryKey,key)) {
        return getUnexpiredValue(e) != null;
      }
    }
  }
  return false;
}
