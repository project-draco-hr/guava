{
  if (count != 0) {
    lock();
    try {
      AtomicReferenceArray<ReferenceEntry<K,V>> table=this.table;
      for (int i=0; i < table.length(); ++i) {
        table.set(i,null);
      }
      clearExpirationQueue();
      ++modCount;
      count=0;
    }
  finally {
      unlock();
    }
  }
}
