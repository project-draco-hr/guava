{
  assertEquals(expected.toString(),actual.toString());
  assertEquals(actual.toString(),expected.hashCode(),actual.hashCode());
  ASSERT.that(actual.getLowerBounds()).has().allFrom(asList(expected.getLowerBounds())).inOrder();
  ASSERT.that(actual.getUpperBounds()).has().allFrom(asList(expected.getUpperBounds())).inOrder();
}
