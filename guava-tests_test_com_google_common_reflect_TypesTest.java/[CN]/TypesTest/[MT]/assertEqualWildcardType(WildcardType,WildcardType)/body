{
  assertEquals(expected.toString(),actual.toString());
  assertEquals(actual.toString(),expected.hashCode(),actual.hashCode());
  assertThat(actual.getLowerBounds()).asList().has().exactlyAs(asList(expected.getLowerBounds())).inOrder();
  assertThat(actual.getUpperBounds()).asList().has().exactlyAs(asList(expected.getUpperBounds())).inOrder();
}
