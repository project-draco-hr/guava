{
  new EqualsTester(Predicates.and(TRUE,isOdd(),NEVER_REACHED)).addEqualObject(Predicates.and(TRUE,isOdd(),NEVER_REACHED)).addNotEqualObject(Predicates.and(isOdd(),NEVER_REACHED,TRUE)).addNotEqualObject(Predicates.and(TRUE)).addNotEqualObject(Predicates.or(TRUE,isOdd(),NEVER_REACHED)).testEquals();
}
