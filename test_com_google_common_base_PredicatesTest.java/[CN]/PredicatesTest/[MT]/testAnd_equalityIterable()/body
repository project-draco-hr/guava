{
  new EqualsTester(Predicates.and(Arrays.asList(TRUE,NEVER_REACHED))).addEqualObject(Predicates.and(Arrays.asList(TRUE,NEVER_REACHED))).addEqualObject(Predicates.and(TRUE,NEVER_REACHED)).addNotEqualObject(Predicates.and(FALSE,NEVER_REACHED)).addNotEqualObject(Predicates.or(TRUE,NEVER_REACHED)).testEquals();
}
