{
  FluentIterable<FluentIterable<Integer>> partitioned=fluent(1,2,3,4,5).partition(3);
  Iterator<FluentIterable<Integer>> iter1=partitioned.iterator();
  ASSERT.that(iter1.next()).hasContentsInOrder(1,2,3);
  Iterator<FluentIterable<Integer>> iter2=partitioned.iterator();
  ASSERT.that(iter1.next()).hasContentsInOrder(4,5);
  assertFalse(iter1.hasNext());
  ASSERT.that(iter2.next()).hasContentsInOrder(1,2,3);
  ASSERT.that(iter2.next()).hasContentsInOrder(4,5);
  assertFalse(iter2.hasNext());
}
