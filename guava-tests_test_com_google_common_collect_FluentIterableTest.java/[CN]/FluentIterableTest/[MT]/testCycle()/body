{
  FluentIterable<String> cycle=FluentIterable.from(asList("a","b")).cycle();
  int howManyChecked=0;
  for (  String string : cycle) {
    String expected=(howManyChecked % 2 == 0) ? "a" : "b";
    assertEquals(expected,string);
    if (howManyChecked++ == 5) {
      break;
    }
  }
  for (  String string : cycle) {
    assertEquals("a",string);
    break;
  }
}
