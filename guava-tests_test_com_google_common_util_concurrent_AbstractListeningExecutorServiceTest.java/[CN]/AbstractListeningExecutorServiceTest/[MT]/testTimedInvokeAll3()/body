{
  ExecutorService e=new DirectExecutorService();
  List<Callable<String>> l=new ArrayList<Callable<String>>();
  l.add(new StringTask());
  l.add(null);
  try {
    e.invokeAll(l,MEDIUM_DELAY_MS,MILLISECONDS);
    shouldThrow();
  }
 catch (  NullPointerException success) {
  }
 finally {
    joinPool(e);
  }
}
