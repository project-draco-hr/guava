{
  lock.lock();
  try {
    if (!hasBeenExecuted) {
      try {
        executor.execute(this);
      }
 catch (      Exception e) {
        logger.log(Level.SEVERE,"Exception while executing listener " + runnable + " with executor "+ executor,e);
      }
      hasBeenExecuted=true;
    }
  }
  finally {
    if (lock.isHeldByCurrentThread()) {
      lock.unlock();
    }
  }
}
