{
  List<MutableNetwork<Integer,String>> testNetworks=ImmutableList.of(NetworkBuilder.directed().<Integer,String>build(),NetworkBuilder.undirected().<Integer,String>build());
  for (  MutableNetwork<Integer,String> graph : testNetworks) {
    graph.addEdge(E11,N1,N1);
    assertThat(oppositeNode(graph,E11,N1)).isEqualTo(N1);
  }
}
