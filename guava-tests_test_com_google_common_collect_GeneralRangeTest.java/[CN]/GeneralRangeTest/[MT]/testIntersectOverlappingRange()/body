{
  GeneralRange<Integer> range=GeneralRange.range(Ordering.natural(),2,OPEN,4,CLOSED);
  assertEquals(GeneralRange.range(Ordering.natural(),3,CLOSED,4,CLOSED),range.intersect(GeneralRange.range(Ordering.natural(),3,CLOSED,5,CLOSED)));
  assertEquals(GeneralRange.range(Ordering.natural(),2,OPEN,3,OPEN),range.intersect(GeneralRange.range(Ordering.natural(),1,OPEN,3,OPEN)));
}
