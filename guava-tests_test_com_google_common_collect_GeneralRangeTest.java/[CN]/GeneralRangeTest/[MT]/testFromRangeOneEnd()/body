{
  for (  BoundType endpointType : BoundType.values()) {
    assertEquals(GeneralRange.upTo(Ordering.natural(),3,endpointType),GeneralRange.from(Ranges.upTo(3,endpointType)));
    assertEquals(GeneralRange.downTo(Ordering.natural(),3,endpointType),GeneralRange.from(Ranges.downTo(3,endpointType)));
  }
}
