{
  hashFunction.hashBytes(new byte[64],0,0);
  try {
    hashFunction.hashBytes(new byte[128],-1,128);
    fail();
  }
 catch (  IndexOutOfBoundsException ok) {
  }
  try {
    hashFunction.hashBytes(new byte[128],64,256);
    fail();
  }
 catch (  IndexOutOfBoundsException ok) {
  }
  try {
    hashFunction.hashBytes(new byte[64],0,-1);
    fail();
  }
 catch (  IndexOutOfBoundsException ok) {
  }
}
