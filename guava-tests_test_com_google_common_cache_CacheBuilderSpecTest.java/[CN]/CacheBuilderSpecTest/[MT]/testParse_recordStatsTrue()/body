{
  CacheBuilderSpec spec=parse("recordStats=true");
  assertTrue(spec.recordStats);
  spec=parse("recordStats=TRUE");
  assertTrue(spec.recordStats);
  spec=parse("recordStats=TrUe");
  assertTrue(spec.recordStats);
  assertCacheBuilderEquivalence(CacheBuilder.newBuilder().recordStats(),CacheBuilder.from(spec));
}
