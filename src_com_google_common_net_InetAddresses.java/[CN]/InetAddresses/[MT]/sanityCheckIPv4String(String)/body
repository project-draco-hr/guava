{
  int count=0;
  for (  String part : SPLITTER.split(ipv4String)) {
    if (++count > 4) {
      return false;
    }
    try {
      if (!part.equals(Integer.toString(Integer.parseInt(part)))) {
        return false;
      }
    }
 catch (    NumberFormatException nfe) {
      return false;
    }
  }
  return count == 4;
}
