{
  TestSuite suite=new TestSuite(SynchronizedBiMapTest.class);
  suite.addTest(BiMapTestSuiteBuilder.using(new SynchTestingBiMapGenerator()).named("Synchronized.biMap[TestBiMap]").withFeatures(CollectionSize.ANY,CollectionFeature.SUPPORTS_ITERATOR_REMOVE,MapFeature.ALLOWS_NULL_KEYS,MapFeature.ALLOWS_NULL_VALUES,MapFeature.GENERAL_PURPOSE,MapFeature.REJECTS_DUPLICATES_AT_CREATION).createTestSuite());
  suite.addTest(BiMapTestSuiteBuilder.using(new SynchronizedHashBiMapGenerator()).named("synchronizedBiMap[HashBiMap]").withFeatures(CollectionSize.ANY,CollectionFeature.SUPPORTS_ITERATOR_REMOVE,MapFeature.ALLOWS_NULL_KEYS,MapFeature.ALLOWS_NULL_VALUES,MapFeature.GENERAL_PURPOSE,MapFeature.REJECTS_DUPLICATES_AT_CREATION,CollectionFeature.SERIALIZABLE).suppressing(BiMapInverseTester.getInverseSameAfterSerializingMethods()).createTestSuite());
  return suite;
}
