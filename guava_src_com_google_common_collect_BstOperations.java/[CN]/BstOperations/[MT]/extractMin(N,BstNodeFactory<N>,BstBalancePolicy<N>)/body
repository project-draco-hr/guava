{
  checkNotNull(root);
  checkNotNull(nodeFactory);
  checkNotNull(balancePolicy);
  if (root.hasChild(LEFT)) {
    BstMutationResult<K,N> subResult=extractMin(root.getChild(LEFT),nodeFactory,balancePolicy);
    return subResult.lift(root,LEFT,nodeFactory,balancePolicy);
  }
  return BstMutationResult.mutationResult(root.getKey(),root,root.childOrNull(RIGHT),BstModificationResult.rebalancingChange(root,null));
}
