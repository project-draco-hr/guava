{
  ByteArrayDataInput in=ByteStreams.newDataInput(BYTES);
  for (int i=0; i < BYTES.length; i++) {
    assertEquals(BYTES[i],in.readUnsignedByte());
  }
  try {
    in.readUnsignedByte();
    fail();
  }
 catch (  IllegalStateException ex) {
    assertTrue(ex.getCause() instanceof EOFException);
  }
}
