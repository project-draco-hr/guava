{
  for (int i=0; i < 10; i++) {
    CountDownLatch countDownLatch=new CountDownLatch(3);
    queue.add(new CountDownRunnable(countDownLatch),executor);
    queue.add(new CountDownRunnable(countDownLatch),executor);
    queue.add(new CountDownRunnable(countDownLatch),executor);
    assertEquals(countDownLatch.getCount(),3L);
    queue.execute();
    assertTrue(countDownLatch.await(1,TimeUnit.SECONDS));
  }
}
