{
  Map<String,String> m=Splitter.on(',').trimResults().withKeyValueSeparator(Splitter.on(':').trimResults()).split("boy  : tom , girl: tina , cat  : kitty , dog: tommy ");
  assertEquals("tom",m.get("boy"));
  assertEquals("tina",m.get("girl"));
  assertEquals("kitty",m.get("cat"));
  assertEquals("tommy",m.get("dog"));
}
