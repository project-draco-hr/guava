{
  checkNotNull(lowerType);
  checkNotNull(upperType);
  Cut<C> lowerBound=(lowerType == BoundType.OPEN) ? new AboveValue<C>(lower) : new BelowValue<C>(lower);
  Cut<C> upperBound=(upperType == BoundType.OPEN) ? new BelowValue<C>(upper) : new AboveValue<C>(upper);
  return create(lowerBound,upperBound);
}
