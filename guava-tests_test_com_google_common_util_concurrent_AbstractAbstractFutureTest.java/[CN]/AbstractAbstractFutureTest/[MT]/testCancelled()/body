{
  assertThat(future.cancel(false)).isTrue();
  assertThat(future.isDone()).isTrue();
  assertThat(future.isCancelled()).isTrue();
  try {
    future.get();
    fail();
  }
 catch (  CancellationException expected) {
  }
  try {
    future.get(0,TimeUnit.SECONDS);
    fail();
  }
 catch (  CancellationException expected) {
  }
}
