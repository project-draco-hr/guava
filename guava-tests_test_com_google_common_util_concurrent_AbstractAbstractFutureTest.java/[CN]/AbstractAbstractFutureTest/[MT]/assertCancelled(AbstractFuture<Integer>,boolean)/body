{
  assertDone(future);
  assertThat(future.isCancelled()).isTrue();
  assertThat(future.wasInterrupted()).isEqualTo(expectWasInterrupted);
  try {
    future.get();
    fail();
  }
 catch (  CancellationException expected) {
  }
  try {
    future.get(0,SECONDS);
    fail();
  }
 catch (  CancellationException expected) {
  }
}
