{
  try {
    long unused=UnsignedLongs.decode("0xfffffffffffffffff");
    fail();
  }
 catch (  NumberFormatException expected) {
  }
  try {
    long unused=UnsignedLongs.decode("-5");
    fail();
  }
 catch (  NumberFormatException expected) {
  }
  try {
    long unused=UnsignedLongs.decode("-0x5");
    fail();
  }
 catch (  NumberFormatException expected) {
  }
  try {
    long unused=UnsignedLongs.decode("-05");
    fail();
  }
 catch (  NumberFormatException expected) {
  }
}
