{
  if (c == 0) {
    return containsZero;
  }
  if (!checkFilter(c)) {
    return false;
  }
  int index=c % table.length;
  if (index < 0) {
    index+=table.length;
  }
  while (true) {
    if (table[index] == 0) {
      return false;
    }
 else     if (table[index] == c) {
      return true;
    }
 else     if (reprobe) {
      index=(index + 1) % table.length;
    }
 else {
      return false;
    }
  }
}
