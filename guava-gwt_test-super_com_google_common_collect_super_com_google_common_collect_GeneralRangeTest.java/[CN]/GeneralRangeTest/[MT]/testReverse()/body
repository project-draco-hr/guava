{
  assertEquals(GeneralRange.all(ORDERING.reverse()),GeneralRange.all(ORDERING).reverse());
  assertEquals(GeneralRange.downTo(ORDERING.reverse(),3,CLOSED),GeneralRange.upTo(ORDERING,3,CLOSED).reverse());
  assertEquals(GeneralRange.upTo(ORDERING.reverse(),3,OPEN),GeneralRange.downTo(ORDERING,3,OPEN).reverse());
  assertEquals(GeneralRange.range(ORDERING.reverse(),5,OPEN,3,CLOSED),GeneralRange.range(ORDERING,3,CLOSED,5,OPEN).reverse());
}
