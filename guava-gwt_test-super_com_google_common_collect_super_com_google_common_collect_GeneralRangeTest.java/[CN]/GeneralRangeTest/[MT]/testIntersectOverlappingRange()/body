{
  GeneralRange<Integer> range=GeneralRange.range(ORDERING,2,OPEN,4,CLOSED);
  assertEquals(GeneralRange.range(ORDERING,3,CLOSED,4,CLOSED),range.intersect(GeneralRange.range(ORDERING,3,CLOSED,5,CLOSED)));
  assertEquals(GeneralRange.range(ORDERING,2,OPEN,3,OPEN),range.intersect(GeneralRange.range(ORDERING,1,OPEN,3,OPEN)));
}
