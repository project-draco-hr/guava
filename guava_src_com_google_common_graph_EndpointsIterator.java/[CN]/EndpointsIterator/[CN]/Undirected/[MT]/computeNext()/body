{
  while (true) {
    while (successorIterator.hasNext()) {
      N otherNode=successorIterator.next();
      if (!visitedNodes.contains(otherNode)) {
        return Endpoints.ofUndirected(node,otherNode);
      }
    }
    visitedNodes.add(node);
    if (!advance()) {
      visitedNodes=null;
      return endOfData();
    }
  }
}
