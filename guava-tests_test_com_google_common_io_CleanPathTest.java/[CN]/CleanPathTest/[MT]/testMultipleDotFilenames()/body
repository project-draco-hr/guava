{
  assertEquals("..a",Files.simplifyPath("..a"));
  assertEquals("/..a",Files.simplifyPath("/..a"));
  assertEquals("/..a/..b",Files.simplifyPath("/..a/..b"));
  assertEquals("/.....a/..b",Files.simplifyPath("/.....a/..b"));
  assertEquals("..../....",Files.simplifyPath("..../...."));
  assertEquals("..a../..b..",Files.simplifyPath("..a../..b.."));
}
