{
  addNode(N1);
  Set<Integer> successors=directedGraph.successors(N1);
  try {
    successors.add(N2);
    fail(ERROR_MODIFIABLE_SET);
  }
 catch (  UnsupportedOperationException e) {
    addEdge(E12,N1,N2);
    assertThat(successors).containsExactlyElementsIn(directedGraph.successors(N1));
  }
}
