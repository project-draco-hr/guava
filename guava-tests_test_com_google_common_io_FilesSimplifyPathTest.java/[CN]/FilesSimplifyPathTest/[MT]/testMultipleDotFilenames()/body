{
  assertEquals("..a",simplifyPath("..a"));
  assertEquals("/..a",simplifyPath("/..a"));
  assertEquals("/..a/..b",simplifyPath("/..a/..b"));
  assertEquals("/.....a/..b",simplifyPath("/.....a/..b"));
  assertEquals("..../....",simplifyPath("..../...."));
  assertEquals("..a../..b..",simplifyPath("..a../..b.."));
}
