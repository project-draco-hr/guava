  new EqualsTester().addEqualityGroup(LENGTH_EQUIVALENCE.wrap("hello"),LENGTH_EQUIVALENCE.wrap("hello"),LENGTH_EQUIVALENCE.wrap("world")).addEqualityGroup(LENGTH_EQUIVALENCE.wrap("hi"),LENGTH_EQUIVALENCE.wrap("yo")).addEqualityGroup(LENGTH_EQUIVALENCE.wrap(null),LENGTH_EQUIVALENCE.wrap(null)).addEqualityGroup(Equivalence.equals().wrap("hello")).addEqualityGroup(Equivalence.equals().wrap(null)).testEquals();
