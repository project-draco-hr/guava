{
  assertThat(EMPTY_PAIRED_STATS.toString()).isEqualTo("PairedStats{xStats=Stats{count=0}, yStats=Stats{count=0}}");
  assertThat(MANY_VALUES_PAIRED_STATS.toString()).isEqualTo("PairedStats{xStats=" + MANY_VALUES_PAIRED_STATS.xStats() + ", yStats="+ MANY_VALUES_PAIRED_STATS.yStats()+ ", populationCovariance="+ MANY_VALUES_PAIRED_STATS.populationCovariance()+ "}");
}
