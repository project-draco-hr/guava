{
  if (computedValue == null) {
    boolean interrupted=false;
    try {
synchronized (this) {
        while (computedValue == null) {
          try {
            wait();
          }
 catch (          InterruptedException ie) {
            interrupted=true;
          }
        }
      }
    }
  finally {
      if (interrupted) {
        Thread.currentThread().interrupt();
      }
    }
  }
  return computedValue.get();
}
