{
  drainRecencyQueue();
  totalWeight+=weight;
  if (map.expires()) {
    long time=map.ticker.read();
    if (map.expiresAfterAccess()) {
      entry.setAccessTime(time);
    }
    if (map.expiresAfterWrite()) {
      entry.setWriteTime(time);
    }
  }
  accessQueue.add(entry);
  writeQueue.add(entry);
}
