{
  if (!valueReference.isLoading()) {
    throw new AssertionError();
  }
  checkState(!Thread.holdsLock(e),"Recursive load");
  try {
    V value=valueReference.waitForValue();
    if (value == null) {
      throw new AssertionError();
    }
    recordRead(e);
    return value;
  }
  finally {
    statsCounter.recordMiss();
  }
}
