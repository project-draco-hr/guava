{
  System.clearProperty(BloomFilter.USE_MITZ32_PROPERTY);
  assertEquals(BloomFilterStrategies.MURMUR128_MITZ_64,BloomFilter.getDefaultStrategyFromSystemProperty());
  System.setProperty(BloomFilter.USE_MITZ32_PROPERTY,"true");
  assertEquals(BloomFilterStrategies.MURMUR128_MITZ_32,BloomFilter.getDefaultStrategyFromSystemProperty());
  System.setProperty(BloomFilter.USE_MITZ32_PROPERTY,"TRUE");
  assertEquals(BloomFilterStrategies.MURMUR128_MITZ_32,BloomFilter.getDefaultStrategyFromSystemProperty());
  System.setProperty(BloomFilter.USE_MITZ32_PROPERTY,"false");
  assertEquals(BloomFilterStrategies.MURMUR128_MITZ_64,BloomFilter.getDefaultStrategyFromSystemProperty());
}
