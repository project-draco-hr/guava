{
  if (index == 0) {
    queue[0]=x;
    return 0;
  }
  int parentIndex=getParentIndex(index);
  E parentElement=elementData(parentIndex);
  if (ordering.compare(parentElement,x) < 0) {
    queue[index]=parentElement;
    queue[parentIndex]=x;
    return parentIndex;
  }
  queue[index]=x;
  return index;
}
