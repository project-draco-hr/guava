{
  return new BSTPathFactory<N,BSTInOrderPath<N>>(){
    @Override public BSTInOrderPath<N> extension(    BSTInOrderPath<N> path,    BSTSide side){
      return BSTInOrderPath.extension(path,side);
    }
    @Override public BSTInOrderPath<N> initialPath(    N root){
      return new BSTInOrderPath<N>(root,null,null);
    }
  }
;
}
