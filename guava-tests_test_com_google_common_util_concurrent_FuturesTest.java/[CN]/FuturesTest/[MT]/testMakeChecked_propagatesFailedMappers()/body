{
  SettableFuture<String> future=SettableFuture.create();
  CheckedFuture<String,TestException> checked=Futures.makeChecked(future,new Function<Exception,TestException>(){
    @Override public TestException apply(    Exception from){
      throw new NullPointerException();
    }
  }
);
  future.setException(new Exception("failed"));
  try {
    checked.checkedGet();
    fail();
  }
 catch (  NullPointerException expected) {
  }
  try {
    checked.checkedGet(5,TimeUnit.SECONDS);
    fail();
  }
 catch (  NullPointerException expected) {
  }
}
