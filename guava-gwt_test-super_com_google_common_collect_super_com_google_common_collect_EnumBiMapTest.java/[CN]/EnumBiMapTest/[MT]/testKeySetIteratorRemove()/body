{
  Map<Currency,Country> map=ImmutableMap.of(Currency.DOLLAR,Country.CANADA,Currency.PESO,Country.CHILE,Currency.FRANC,Country.SWITZERLAND);
  EnumBiMap<Currency,Country> bimap=EnumBiMap.create(map);
  Iterator<Currency> iter=bimap.keySet().iterator();
  assertEquals(Currency.DOLLAR,iter.next());
  iter.remove();
  assertThat(bimap.keySet()).has().exactly(Currency.FRANC,Currency.PESO).inOrder();
  assertThat(bimap.values()).has().exactly(Country.SWITZERLAND,Country.CHILE).inOrder();
  assertThat(bimap.inverse().keySet()).has().exactly(Country.CHILE,Country.SWITZERLAND).inOrder();
  assertThat(bimap.inverse().values()).has().exactly(Currency.PESO,Currency.FRANC).inOrder();
}
