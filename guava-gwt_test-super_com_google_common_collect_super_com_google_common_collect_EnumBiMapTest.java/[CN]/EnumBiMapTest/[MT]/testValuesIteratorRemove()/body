{
  Map<Currency,Country> map=ImmutableMap.of(Currency.DOLLAR,Country.CANADA,Currency.PESO,Country.CHILE,Currency.FRANC,Country.SWITZERLAND);
  EnumBiMap<Currency,Country> bimap=EnumBiMap.create(map);
  Iterator<Currency> iter=bimap.keySet().iterator();
  assertEquals(Currency.DOLLAR,iter.next());
  assertEquals(Currency.FRANC,iter.next());
  iter.remove();
  ASSERT.that(bimap.keySet()).has().allOf(Currency.DOLLAR,Currency.PESO).inOrder();
  ASSERT.that(bimap.values()).has().allOf(Country.CANADA,Country.CHILE).inOrder();
  ASSERT.that(bimap.inverse().keySet()).has().allOf(Country.CANADA,Country.CHILE).inOrder();
  ASSERT.that(bimap.inverse().values()).has().allOf(Currency.DOLLAR,Currency.PESO).inOrder();
}
