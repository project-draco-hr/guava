{
  return new UnmodifiableIterator<N>(){
    private int pos=0;
    @Override public boolean hasNext(){
      return pos < 2;
    }
    @Override public N next(){
switch (pos++) {
case 0:
        return nodeA;
case 1:
      return nodeB;
default :
    pos=2;
  throw new NoSuchElementException();
}
}
}
;
}
