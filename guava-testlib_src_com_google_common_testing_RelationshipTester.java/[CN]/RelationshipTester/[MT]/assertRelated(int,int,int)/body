{
  ImmutableList<T> group=groups.get(groupNumber);
  T item=group.get(itemNumber);
  T related=group.get(relatedItemNumber);
  try {
    assertion.assertRelated(item,related);
  }
 catch (  AssertionFailedError e) {
    throw new AssertionFailedError(e.getMessage().replace("$ITEM",itemReporter.reportItem(new Item(item,groupNumber,itemNumber))).replace("$RELATED",itemReporter.reportItem(new Item(related,groupNumber,relatedItemNumber))));
  }
}
