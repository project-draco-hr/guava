{
  IOException expected=new IOException("expected");
  expect(delegate.checkedGet()).andThrow(expected).times(2);
  replay(delegate);
  try {
    delegate.checkedGet();
    fail();
  }
 catch (  IOException e) {
    assertEquals(expected.getMessage(),e.getMessage());
  }
  try {
    simple.checkedGet();
    fail();
  }
 catch (  IOException e) {
    assertEquals(expected.getMessage(),e.getMessage());
  }
  verify(delegate);
}
