{
  expect(delegate.checkedGet(100,TIME_UNIT)).andThrow(new TimeoutException()).times(2);
  replay(delegate);
  try {
    forwarded.checkedGet(100,TIME_UNIT);
    fail();
  }
 catch (  TimeoutException expected) {
  }
  try {
    simple.checkedGet(100,TIME_UNIT);
    fail();
  }
 catch (  TimeoutException expected) {
  }
  verify(delegate);
}
