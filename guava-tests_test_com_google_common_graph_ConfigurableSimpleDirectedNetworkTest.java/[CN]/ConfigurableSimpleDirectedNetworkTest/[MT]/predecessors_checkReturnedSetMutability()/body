{
  addNode(N2);
  Set<Integer> predecessors=network.predecessors(N2);
  try {
    predecessors.add(N1);
    fail(ERROR_MODIFIABLE_COLLECTION);
  }
 catch (  UnsupportedOperationException e) {
    addEdge(E12,N1,N2);
    assertThat(network.predecessors(N2)).containsExactlyElementsIn(predecessors);
  }
}
