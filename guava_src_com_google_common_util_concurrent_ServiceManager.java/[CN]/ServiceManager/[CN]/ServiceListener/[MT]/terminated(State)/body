{
  logger.info("Service " + service + " has terminated. Previous state was "+ from+ " state.");
  state.monitor.enter();
  try {
    if (from == State.NEW) {
      startTimer();
      finishedStarting(false);
    }
    state.serviceTerminated(service);
  }
  finally {
    state.monitor.leave();
    state.executeListeners();
  }
}
