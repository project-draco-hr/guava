{
  if (c == null)   throw new NullPointerException();
  if (c == this)   throw new IllegalArgumentException();
  final E[] items=this.items;
  final Monitor monitor=this.monitor;
  monitor.enter();
  try {
    int i=takeIndex;
    int n=0;
    int max=count;
    while (n < max) {
      c.add(items[i]);
      items[i]=null;
      i=inc(i);
      ++n;
    }
    if (n > 0) {
      count=0;
      putIndex=0;
      takeIndex=0;
    }
    return n;
  }
  finally {
    monitor.leave();
  }
}
