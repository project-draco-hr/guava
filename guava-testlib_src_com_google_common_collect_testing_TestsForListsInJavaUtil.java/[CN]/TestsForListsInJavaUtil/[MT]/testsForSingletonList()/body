{
  return ListTestSuiteBuilder.using(new TestStringListGenerator(){
    @Override public List<String> create(    String[] elements){
      return Collections.singletonList(elements[0]);
    }
  }
).named("singletonList").withFeatures(CollectionFeature.NONE,CollectionFeature.ALLOWS_NULL_VALUES,CollectionSize.ONE).suppressing(suppressForSingletonList()).createTestSuite();
}
