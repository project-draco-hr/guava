{
  return ListTestSuiteBuilder.using(new TestStringListGenerator(){
    @Override public List<String> create(    String[] elements){
      return Arrays.asList(elements.clone());
    }
  }
).named("Arrays.asList").withFeatures(ListFeature.SUPPORTS_SET,CollectionFeature.ALLOWS_NULL_VALUES,CollectionSize.ANY).suppressing(suppressForArraysAsList()).createTestSuite();
}
