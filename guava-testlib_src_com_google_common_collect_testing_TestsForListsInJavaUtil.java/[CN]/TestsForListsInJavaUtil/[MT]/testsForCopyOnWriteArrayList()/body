{
  return ListTestSuiteBuilder.using(new TestStringListGenerator(){
    @Override public List<String> create(    String[] elements){
      return new CopyOnWriteArrayList<String>(MinimalCollection.of(elements));
    }
  }
).named("CopyOnWriteArrayList").withFeatures(ListFeature.GENERAL_PURPOSE,CollectionFeature.SERIALIZABLE,CollectionFeature.ALLOWS_NULL_VALUES,CollectionSize.ANY).suppressing(suppressForCopyOnWriteArrayList()).createTestSuite();
}
