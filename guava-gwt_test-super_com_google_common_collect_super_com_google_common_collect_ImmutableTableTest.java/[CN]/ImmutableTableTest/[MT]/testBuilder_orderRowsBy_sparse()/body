{
  ImmutableTable.Builder<Character,Integer,String> builder=ImmutableTable.builder();
  builder.orderRowsBy(Ordering.natural());
  builder.put('x',2,"foo");
  builder.put('r',1,"bar");
  builder.put('c',3,"baz");
  builder.put('b',7,"cat");
  builder.put('e',5,"dog");
  builder.put('c',0,"axe");
  builder.put('e',3,"tub");
  builder.put('r',4,"foo");
  builder.put('x',5,"bar");
  Table<Character,Integer,String> table=builder.build();
  assertThat(table.rowKeySet()).has().exactly('b','c','e','r','x').inOrder();
  assertThat(table.column(5).keySet()).has().exactly('e','x').inOrder();
}
